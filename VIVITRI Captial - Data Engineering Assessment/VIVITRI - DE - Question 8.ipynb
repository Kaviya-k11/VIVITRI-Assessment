     "text": [
      "Enter the number :4\n",
      "1 * 2 * 3 * 4 \n",
      "1 * 2 * 3 \n",
      "1 * 2 \n",
      "1 \n"
     ]
    }
   ],
   "source": [
    "n=int(input(\"Enter the number :\"))\n",
    "for i in range(n,0,-1):\n",
    "    num=1\n",
    "    for j in range(i,0,-1):\n",
    "        if j!=i:\n",
    "            print(\"* \",end=\"\")\n",
    "        print(num,end=\" \")\n",
    "        num=num+1\n",
    "    print()"
   ]
  }
